class Click{
  //holds x and y position where the cursor has been clicked
  private int x;
  private int y;
  
  Click(int x, int y)
  {
    this.x = x;
    this.y = y;
  }
  
  public int getX()
  {
    return x;
  }
  
  public int getY()
  {
    return y;
  }
  
  

}


  
